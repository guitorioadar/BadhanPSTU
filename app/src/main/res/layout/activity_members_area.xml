<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.vaidoos.guitorio.badhanpstu.MembersArea">


    <Button
        android:id="@+id/btnMembersSrch"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="187dp"
        android:text="Search"
        android:onClick="btnMembersSrch"/>

    <Button
        android:id="@+id/btnRegisterDonor"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/btnMembersSrch"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="40dp"
        android:text="Register Donors"
        android:onClick="btnRegisterDonor"/>
</RelativeLayout>
